    .global _ceil
_ceil .cproc var

        .reg cnt, cst1, cst2, result
  
        MVK 0x0000FFFF, cst1    ;cst1=0x0000FFFF
        MVKH 0x0000FFFF, cst1
        MVK 0x00010000, cst2    ;cst2=0x00010000
        MVKH 0x00010000, cst2

        AND var, cst1, cnt
        CLR var, 0, 15, result
[!cnt]  ADD result, cst2, result
        
    .return result
    .endproc